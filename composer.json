{
    "name": "sierratecnologia/payment-laravel",
    "description": "Laravel Cashier provides an expressive, fluent interface to SierraTecnologia's subscription billing services.",
    "keywords": ["laravel", "sierratecnologia", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sierratecnologia/cashier/issues",
        "source": "https://github.com/sierratecnologia/cashier"
    },
    "authors": [
        {
            "name": "Ricardo Sierra",
            "email": "contato@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "~5.8.0|~5.9.0",
        "illuminate/database": "~5.8.0|~5.9.0",
        "illuminate/http": "~5.8.0|~5.9.0",
        "illuminate/routing": "~5.8.0|~5.9.0",
        "illuminate/support": "~5.8.0|~5.9.0",
        "illuminate/view": "~5.8.0|~5.9.0",
        "nesbot/carbon": "^1.26.3|^2.0",
        "sierratecnologia/payment-php": "dev-master",
        "symfony/http-kernel": "^4.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "SierraTecnologia\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SierraTecnologia\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.0-dev"
        },
        "laravel": {
            "providers": [
                "SierraTecnologia\\Cashier\\CashierServiceProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
